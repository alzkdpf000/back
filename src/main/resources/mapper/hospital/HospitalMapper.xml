<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.back.mapper.hospital.HospitalMapper">
    <insert id="insertHospital" parameterType="com.example.back.domain.hospital.HospitalVO" useGeneratedKeys="true" keyProperty="id">
        insert into tbl_hospital(hospital_name, hospital_phone, hospital_status)
        values (#{hospitalName}, #{hospitalPhone}, #{hospitalStatus})
    </insert>
</mapper>